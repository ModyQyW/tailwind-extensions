{
  "name": "@modyqyw/tailwind-presets",
  "version": "0.17.1",
  "description": "Tailwind presets.",
  "keywords": [
    "tailwind",
    "tailwindcss",
    "preset",
    "ant-design",
    "element-plus",
    "miniprogram",
    "miniprogram-preflight"
  ],
  "homepage": "https://github.com/ModyQyW/tailwind-presets#readme",
  "bugs": {
    "url": "https://github.com/ModyQyW/tailwind-presets/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ModyQyW/tailwind-presets.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./dist/*"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node",
    "check:types": "tsc --noEmit",
    "commit": "cz",
    "lint": "run-p lint:eslint lint:markdownlint check:types",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx --ignore-path=.gitignore --cache",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release-it": "release-it"
  },
  "dependencies": {
    "@ant-design/colors": "^6.0.0",
    "@types/color": "^3.0.3",
    "color": "^4.2.3",
    "tailwindcss-theme-swapper": "^0.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/prompt": "^17.1.2",
    "@release-it/conventional-changelog": "^5.1.0",
    "@tsconfig/node16": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "commitizen": "^4.2.5",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.0.3",
    "markdownlint-cli": "^0.32.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "release-it": "^15.4.2",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "tailwindcss": "^3.1.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
